{
    "builds": [
        {
            "src": "techx/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "100mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "techx/wsgi.py"
        }
    ],
    "buildCommand": "pip install -r requirements.txt --no-cache-dir",
    "env": {
        "MYSQLCLIENT_CFLAGS": "-I/usr/include/mysql",
        "MYSQLCLIENT_LDFLAGS": "-L/usr/lib/mysql -lmysqlclient"
    }
}