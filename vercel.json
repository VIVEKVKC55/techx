{
    "builds": [
        {
            "src": "techx/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "techx/wsgi.py"
        }
    ],
    "buildCommand": "apt-get update && apt-get install -y default-libmysqlclient-dev && vercel build",
    "env": {
        "MYSQLCLIENT_CFLAGS": "-I/usr/include/mysql",
        "MYSQLCLIENT_LDFLAGS": "-L/usr/lib/mysql -lmysqlclient"
    }
}